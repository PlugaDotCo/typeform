{
  "name": "Typeform",
  "app_id": "typeform",
  "color": "#343A40",
  "description": {
    "pt_BR": "Typeform é um serviço online que permite a criação de elegantes formulários, que podem ser usados para NPS, Pesquisa de Mercado, Comportamento do Consumidor e muito mais!",
    "en": "Typeform is an online service that allows you to create dashing forms, which may be used for NPS, Market Research, Consumer Behavior, and much more!"
  },
  "website": "https://www.typeform.com/",
  "signup_url": "https://admin.typeform.com/login/",
  "api_base_uri": {
    "production": {
      "uri": "https://api.typeform.com",
      "label": "Produção"
    }
  },
  "authentication": {
    "type": "oauth_2",
    "oauth_access_fields": {
      "client_id": "client_id",
      "client_secret": "client_secret",
      "authorize_url": "https://api.typeform.com/oauth/authorize",
      "access_token_url": "https://api.typeform.com/oauth/token",
      "redirect_uri": "https://manage.pluga.co/oauth2/typeform",
      "access_token_placement": "header_bearer",
      "scope": [
        "forms:read"
      ],
      "response_type": "code"
    },
    "ping_request": {
      "method_name": "/forms"
    }
  }
}
